    "android": {
      "package": "com.apptienda.shop",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/favpngSC01.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDHxnof3QneAGFyhDm-Q0c8gKxI7DPEf4w"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
